<p id="notice"><%= notice %></p>

<h1>Listings</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Status</th>
      <th>Owner</th>
      <th>User</th>
      <th>City</th>
      <th>Street</th>
      <th>Landmark</th>
      <th>Phonenumber</th>
      <th>Longtitude</th>
      <th>Latitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.name %></td>
        <td><%= listing.description %></td>
        <td><%= listing.status %></td>
        <td><%= listing.owner %></td>
        <td><%= listing.user %></td>
        <td><%= listing.city %></td>
        <td><%= listing.street %></td>
        <td><%= listing.landmark %></td>
        <td><%= listing.phonenumber %></td>
        <td><%= listing.longtitude %></td>
        <td><%= listing.latitude %></td>
        <td><%= link_to 'Show', listing %></td>
        <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
        <td><%= link_to 'Destroy', listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listing', new_listing_path %>
